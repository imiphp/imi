{
    "name": "imiphp/imi-apidoc",
    "type": "library",
    "license": "MulanPSL-2.0",
    "description": "imi 框架 HTTP API 文档生成器，支持 Swagger！",
    "require": {
        "zircote/swagger-php": "^3.0|~4.7.0",
        "doctrine/annotations": "^1.7"
    },
    "require-dev": {
        "swoole/ide-helper": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Imi\\ApiDoc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiDocApp\\": "example/",
            "Imi\\ApiDoc\\Test\\": "tests/",
            "Imi\\Server\\ApiDoc\\": "src/imi/Server/ApiDoc/"
        },
        "files": [
            "../../../vendor/autoload.php"
        ]
    },
    "scripts": {},
    "extra": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yurunsoft/ide-helper": true
        }
    }
}